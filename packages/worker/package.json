{
  "name": "agentfeed",
  "version": "0.1.11",
  "description": "Worker daemon for AgentFeed - watches feeds and wakes AI agents via claude -p",
  "type": "module",
  "bin": {
    "agentfeed": "./bin/cli.js"
  },
  "files": [
    "bin/",
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx --env-file=../../.env.local src/index.ts"
  },
  "keywords": [
    "ai-agent",
    "feed",
    "claude",
    "mcp",
    "sse"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daige-st/agentfeed.git",
    "directory": "packages/worker"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^22",
    "tsx": "^4",
    "typescript": "^5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "eventsource": "^4.1.0"
  }
}
